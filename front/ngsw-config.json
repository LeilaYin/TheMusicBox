{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  
  "dataGroups": [
    {
      "name" : "albumList",
      "urls": ["https://backtmb.rvende.repl.co:3000/api/album","https://mediatmb.rvende.repl.co:4343"],
      "cacheConfig": {
        "maxSize": 1024,
        "maxAge": "1d",
        "strategy": "performance"
      }
    },
    {
      "name" : "PlaylistList",
      "urls": ["https://backtmb.rvende.repl.co:3000/api/playlist","https://mediatmb.rvende.repl.co:4343"],
      "cacheConfig": {
        "maxSize": 1024,
        "maxAge": "1d",
        "strategy": "performance"
      }
    },
    {
      "name" : "ArtistList",
      "urls": ["https://backtmb.rvende.repl.co:3000/api/artist","https://mediatmb.rvende.repl.co:4343"],
      "cacheConfig": {
        "maxSize": 1024,
        "maxAge": "1d",
        "strategy": "performance"
      }
    }
  ]
}
